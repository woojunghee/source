            end if;
        else
            dout_i1  <= (others => ( others=>'0' ));
            dout_q1  <= (others => ( others=>'0' ));
        end if;
    end if;
end if;
end process;


process(clk_122p88)
begin
if rising_edge(clk_122p88) then
    if (rst_122p88 = '1') then
        dout_i2  <= (others => ( others=>'0' ));
        dout_q2  <= (others => ( others=>'0' ));
    else
        if ( link_up(0) = '1' and link_up(1) = '1') then
            if (vendor_info(0) /= "00" and vendor_info(1) /= "00" and vendor_info(2) = "00" and vendor_info(3) /= "00") then
                dout_i2  <= dcm_din_i;
