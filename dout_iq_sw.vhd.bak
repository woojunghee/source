            elsif (vendor_info(2) = "10" and vendor_info(3) /= "10") then
                dout_i2  <= rm_din_i;
                dout_q2  <= rm_din_q;
            elsif (vendor_info(2) = "11" and vendor_info(3) /= "11") then
