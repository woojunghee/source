            elsif (vendor_info(2) /= "10" and vendor_info(3) = "10") then
                dout_i3  <= rm_din_i;
                dout_q3  <= rm_din_q;
            elsif (vendor_info(2) /= "11" and vendor_info(3) = "11") then
                dout_i3  <= sb_din_i;
                dout_q3  <= sb_din_q;
            else
                dout_i3  <= (others => ( others=>'0' ));
                dout_q3  <= (others => ( others=>'0' ));
            end if;
        else
            dout_i3  <= (others => ( others=>'0' ));
            dout_q3  <= (others => ( others=>'0' ));
        end if;
    end if;
end if;
end process;





end beh;




